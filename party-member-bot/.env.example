# Discord Bot Token - Get from Discord Developer Portal
DISCORD_TOKEN=your_bot_token_here

# Your Discord User ID - The bot will only respond to DMs from this user
OWNER_ID=your_discord_user_id_here

# RPG DM Bot User ID - The bot ID of the RPG Dungeon Master bot
DM_BOT_ID=dm_bot_user_id_here

# Character name (optional, can be set via interview)
CHARACTER_NAME=Adventurer

# Path to store character data
DATA_PATH=data/character.json
